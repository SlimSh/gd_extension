{
    "name": "Den Helper",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": [
      "storage"
    ],
    "background" : {
        "scripts" : ["bundle.js"],
        "persistent": false
    },
    "browser_action" : {
        "default_popup": "index.html"
    }
}